{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/index.js"
    }
  ],
  "hooks": {
    "build": {
      "commands": ["bun build src/index.ts --outdir dist --target node"]
    }
  }
}
